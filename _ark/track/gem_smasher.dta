(RndDir
   (types
      (gem_smasher
         (editor
            (reset
               script
               (script
                  {$this reset}))
            (color
               symbol
               (list
                  (green red yellow blue orange))
               (post_sync
                  {$this
                     set_color
                     [color]}))
            (glow
               bool
               (post_sync
                  {$this
                     set_glow
                     [glow]}))
            (in_danger
               bool
               (post_sync
                  {$this
                     set_danger
                     [in_danger]}))
            (label
               string
               (post_sync
                  {$this
                     set_label
                     [label]}))
            (do_hit
               script
               (script
                  {$this hit}))
            (do_hit_bonus
               script
               (script
                  {$this hit_bonus}))
            (do_big_rock_hit
               script
               (script
                  {$this coda_hit}))
            (do_hit_fill
               script
               (script
                  {$this
                     hit_fill_vel
                     [test_velocity]}))
            (do_hit_drum
               script
               (script
                  {$this hit_drum}))
            (do_hit_cymbal
               script
               (script
                  {$this hit_cymbal}))
            (do_miss
               script
               (script
                  {$this miss}))
            (do_finger_tap
               script
               (script
                  {$this finger_tap}))
            (do_fing_tap_bonus
               script
               (script
                  {$this finger_tap_bonus}))
            (burn
               script
               (script
                  {$this burn}))
            (big_rock_burn
               script
               (script
                  {$this coda_burn}))
            (burn_bonus
               script
               (script
                  {$this burn_bonus}))
            (stop_burn
               script
               (script
                  {$this stop_burn}))
            (combo
               script
               (script
                  {$this combo}))
            (last_note
               script
               (script
                  {$this last_note}))
            (test_velocity
               int
               (range 0 10)
               (help
                  "velocity of fill hit from soft to loud"))
            (velocity_fill_triggers
               (array
                  object
                  (class EventTrigger))
               (help
                  "fill hits by velocity from lightest to hardest"))
            (velocity_fill_triggers_3p
               (array
                  object
                  (class EventTrigger))
               (help
                  "fill hits by velocity from lightest to hardest for 3 players"))
            (hit_lego_stud
               script
               (script
                  {$this hit_lego_stud})))
         (glow FALSE)
         (in_danger FALSE)
         (label
            "")
         (color green proxy_save)
         (burning FALSE)
         (base_frame 0)
         (test_velocity 0)
         (velocity_fill_triggers
            ())
         (velocity_fill_triggers_3p
            ())
         (last_set_color '')
         (reset
            {$this
               set_color
               [color]}
            {if
               {exists reset.trig}
               {reset.trig trigger}}
            {set
               [burning]
               FALSE}
            {if
               {exists LOD.anim}
               {LOD.anim set_frame $track_LOD}})
         (set_color
            ($in_color)
            {if
               {!=
                  [last_set_color]
                  $in_color}
               {do
                  ($slot)
                  ($frame)
                  {set
                     [color]
                     $in_color}
                  {switch
                     $in_color
                     (green
                        {set $slot 0})
                     (red
                        {set $slot 1})
                     (yellow
                        {set $slot 2})
                     (blue
                        {set $slot 3})
                     (orange
                        {set $slot 4})}
                  {set $frame $slot}
                  {if
                     {exists particle_slot_colors.anim}
                     {particle_slot_colors.anim set_frame $frame}}
                  {set
                     [base_frame]
                     {'*' 20 $frame}}
                  {if
                     {exists smasher_glow.anim}
                     {smasher_glow.anim
                        set_frame
                        [base_frame]}}
                  {$this set_launcher_color broken_glass_squares.ml}
                  {$this set_launcher_color broken_glass_triangles.ml}
                  {$this set_launcher_color radial_debris.ml}}
               {set
                  [last_set_color]
                  $in_color}})
         (set_launcher_color
            ($launcher)
            {if
               {exists $launcher}
               {do
                  ($part
                     {{object $launcher}
                        get
                        part})
                  {set
                     $part
                     {$this
                        find
                        {$part name}}}
                  {$launcher set use_start_color TRUE}
                  {$launcher
                     set
                     start_color
                     {$part get start_color_high}}
                  {$launcher
                     set
                     start_alpha
                     {$part get start_alpha_high}}
                  #ifndef HX_OG
                  {$launcher set use_mid_color TRUE}
                  {$launcher
                     set
                     mid_color
                     {$part get mid_color_high}}
                  {$launcher
                     set
                     mid_alpha
                     {$part get mid_alpha_high}}
                  #endif
                  {$launcher set use_end_color TRUE}
                  {$launcher
                     set
                     end_color
                     {$part get end_color_high}}
                  {$launcher
                     set
                     end_alpha
                     {$part get end_alpha_high}}}})
         (set_glow
            ($in_state)
            {set
               [glow]
               $in_state}
            {if
               {exists smasher_glow.anim}
               {if_else
                  [glow]
                  {smasher_glow.anim
                     animate
                     (range
                        {'+'
                           [base_frame]
                           19}
                        {'+'
                           [base_frame]
                           19})}
                  {smasher_glow.anim
                     animate
                     (range
                        [base_frame]
                        [base_frame])}}})
         (set_label
            ($in_val))
         (hit
            {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {hit.trig trigger}}
            {if {modifier_mgr is_modifier_active mod_flashbang} 
			{big_rock_hit.trig trigger}
			{big_rock_burn.trig trigger}
			{burn.trig trigger}
			{$this burn_bonus}
			{$this burn}
			{set [burning] TRUE}
			})
         (hit_3p
            {hit_3p.trig trigger})
         (hit_cymbal
            {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {hit.trig trigger}}
            {if {modifier_mgr is_modifier_active mod_flashbang} 
			{big_rock_hit.trig trigger}
			{big_rock_burn.trig trigger}
			{burn.trig trigger}
			{$this burn_bonus}
			{$this burn}
			{set [burning] TRUE}
			})
         (hit_cymbal_3p
            {hit_cymbals_3p.trig trigger})
         (hit_drum
            {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {hit_drums.trig trigger}}
            {if {modifier_mgr is_modifier_active mod_flashbang} 
			{big_rock_hit.trig trigger}
			{big_rock_burn.trig trigger}
			{burn.trig trigger}
			{$this burn_bonus}
			{$this burn}
			{set [burning] TRUE}
			})
         (hit_drum_3p
            {hit_drums_3p.trig trigger})
         (coda_hit
            {$this hit}
            {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {big_rock_hit.trig trigger}})
         (coda_hit_3p
            {$this hit_3p}
            {big_rock_hit_3p.trig trigger})
         (hit_bonus
            {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {hit_bonus.trig trigger}}
            {if {modifier_mgr is_modifier_active mod_flashbang} 
			{big_rock_hit.trig trigger}
			{big_rock_burn.trig trigger}
			{burn.trig trigger}
			{$this burn_bonus}
			{$this burn}
			{set [burning] TRUE}
			})
         (hit_bonus_3p
            {hit_bonus_3p.trig trigger})
         (hit_fill
            {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {hit_fill.trig trigger}}
            {if {modifier_mgr is_modifier_active mod_flashbang} 
			{big_rock_hit.trig trigger}
			{big_rock_burn.trig trigger}
			{burn.trig trigger}
			{$this burn_bonus}
			{$this burn}
			{set [burning] TRUE}
			})
         (hit_fill_3p
            {hit_fill_3p.trig trigger})
         (hit_fill_vel
            ($velocity)
            {if
               {== $velocity 0}
               {set $velocity 6}}
            {if_else
               {<
                  $velocity
                  {size
                     [velocity_fill_triggers]}}
               {do
                  ($trig
                     {elem
                        [velocity_fill_triggers]
                        $velocity})
                  {if_else
                     {!= $trig ''}
                     {$trig trigger}
                     {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {hit_fill.trig trigger}}}}
               {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {hit_fill.trig trigger}}})
         (hit_fill_vel_3p
            ($velocity)
            {if
               {== $velocity 0}
               {set $velocity 6}}
            {if_else
               {<
                  $velocity
                  {size
                     [velocity_fill_triggers_3p]}}
               {do
                  ($trig
                     {elem
                        [velocity_fill_triggers_3p]
                        $velocity})
                  {if_else
                     {!= $trig ''}
                     {$trig trigger}
                     {hit_fill_3p.trig trigger}}}
               {hit_fill_3p.trig trigger}})
         (finger_tap
            {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {finger_tap.trig trigger}}
            {if {modifier_mgr is_modifier_active mod_flashbang} 
			{big_rock_hit.trig trigger}
			{big_rock_burn.trig trigger}
			{burn.trig trigger}
			{$this burn_bonus}
			{$this burn}
			{set [burning] TRUE}
			})
         (finger_tap_bonus
            {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {finger_tap_bonus.trig trigger}}
            {if {modifier_mgr is_modifier_active mod_flashbang} 
			{big_rock_hit.trig trigger}
			{big_rock_burn.trig trigger}
			{burn.trig trigger}
			{$this burn_bonus}
			{$this burn}
			{set [burning] TRUE}
			})
         (miss
            {miss.trig trigger}
            {$this stop_burn})
         (burn
            {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {burn.trig trigger}}
            {unless {modifier_mgr is_modifier_active mod_noflames} {set
               [burning]
               TRUE}})
         (coda_burn
            {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {big_rock_burn.trig trigger}}
            {unless {modifier_mgr is_modifier_active mod_noflames} {set
               [burning]
               TRUE}})
         (burn_bonus
            {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {burn_bonus.trig trigger}}
            {unless {modifier_mgr is_modifier_active mod_noflames} {set
               [burning]
               TRUE}})
         (stop_burn
            {unless {modifier_mgr is_modifier_active mod_flashbang} 
            {if
               [burning]
               {stop_burn.trig trigger}
               {set
                  [burning]
                  FALSE}}})
         (combo
            {if
               {exists spotlight_phrase_success.trig}
               {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {spotlight_phrase_success.trig trigger}}})
         (combo_3p
            {if
               {exists spotlight_phrase_success_3p.trig}
               {spotlight_phrase_success_3p.trig trigger}})
         (last_note
            {if
               {exists BRE_final_gem_hit.trig}
               {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {BRE_final_gem_hit.trig trigger}}})
         (last_note_3p
            {if
               {exists BRE_final_gem_hit_3p.trig}
               {BRE_final_gem_hit_3p.trig trigger}})
         (drum_fill_complete
            {if
               {exists drumfill_complete_hit.trig}
               {if_else {modifier_mgr is_modifier_active mod_noflames} {miss.trig trigger} {drumfill_complete_hit.trig trigger}}})
         (drum_fill_complete_3p
            {if
               {exists drumfill_complete_hit_3p.trig}
               {drumfill_complete_hit_3p.trig trigger}})
         (set_danger
            ($in_state))
         (hit_lego_stud
            {if
               {exists LegoStud_Hit.trig}
               {LegoStud_Hit.trig trigger}}))))