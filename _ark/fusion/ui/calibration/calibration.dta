{func
   increment_to_1mult
   ($value)
   {'*'
      {'+'
         {floor
            {/ $value 1.0}}
         1}
      1}}
{func
   decrement_to_1mult
   ($value)
   {'*'
      {-
         {ceil
            {/ $value 1.0}}
         1}
      1}}
{new
   UIPanel
   cal_background_panel
   (file cal_background.milo)
   (parent
      {ui current_screen})
   (enter
      {meta music_stop}
      {platform_mgr disable_xmp})
   (exit
      {meta music_start}
      {platform_mgr enable_xmp}
      {input_mgr set_user ''})
   (BUTTON_DOWN_MSG
      {switch
         $action
         (kAction_Cancel
            {if
               {&&
                  {[parent]
                     has
                     (cancel_screen)}
                  {!=
                     ''
                     {[parent]
                        get
                        (cancel_screen)}}}
               {synth play button_back}
               {ui
                  goto_screen
                  {[parent]
                     get
                     (cancel_screen)}}})
         (kAction_Confirm
            {if
               {&&
                  {[parent]
                     has
                     (confirm_screen)}
                  {!=
                     ''
                     {[parent]
                        get
                        (confirm_screen)}}}
               {synth play button_select}
               {ui
                  goto_screen
                  {[parent]
                     get
                     (confirm_screen)}}})
         (kAction_Option
            {if
               {&&
                  {[parent]
                     has
                     (option1_screen)}
                  {!=
                     ''
                     {[parent]
                        get
                        (option1_screen)}}}
               {synth play button_select}
               {ui
                  goto_screen
                  {[parent]
                     get
                     (option1_screen)}}})
         #ifdef HX_WII
         (kAction_Option1
            {if
               {&&
                  {[parent]
                     has
                     (option1_screen)}
                  {!=
                     ''
                     {[parent]
                        get
                        (option1_screen)}}}
               {synth play button_select}
               {ui
                  goto_screen
                  {[parent]
                     get
                     (option1_screen)}}})
         #endif
         kDataUnhandled})}
#define CAL_BASIC_PANEL
((enter
